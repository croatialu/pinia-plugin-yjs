{
  "name": "pinia-plugin-yjs",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.14.0",
  "description": "pinia plugin that enables sharing of state between clients via Yjs",
  "author": "croatialu<wuli.croatia@foxmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/croatialu",
  "homepage": "https://github.com/croatialu/pinia-plugin-yjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croatialu/pinia-plugin-yjs.git"
  },
  "bugs": "https://github.com/croatialu/pinia-plugin-yjs/issues",
  "keywords": [
    "yjs",
    "pinia",
    "state-management",
    "middleware",
    "peer-to-peer",
    "p2p",
    "distributed",
    "local-first",
    "offline-first",
    "crdt",
    "decentralized",
    "shared-editing",
    "realtime"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "pinia": "^2.0.0",
    "vue": "^3.0.0",
    "yjs": "^13.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.10.7",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pinia": "^2.1.7",
    "pnpm": "^8.14.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3",
    "vue": "^3.4.19",
    "yjs": "^13.6.12"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
